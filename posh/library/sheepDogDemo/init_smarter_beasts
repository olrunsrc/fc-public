# This agent initialisation file initialises 2 dogs and 10 sheep

# -----------------
# 2 Dogs
# Each dog uses the dog.lap plan.
# -----------------

[sheep-dog]
MASON.name = Todor
MASON.fieldNames = dogs, agents
MASON.display = kite, red, 4, Todor
PanicFlocking.MINDISTANCE = 15

[sheep-dog]
MASON.name = Guinness
MASON.fieldNames = dogs, agents
MASON.display = kite, red, 4, Guinness
